{
  "compilerOptions": {
    "declaration": true,
    "strictNullChecks": true,
    "target": "es6",
    "outDir": "lib",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "lib": [
      "es2015",
    ],
    "rootDir": "src",
    "baseUrl": "./src/"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}
